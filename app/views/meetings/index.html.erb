<p id="notice"><%= notice %></p>

<h1>Meetings</h1>

<table>
  <thead>
    <tr>
      <th>Matcher 1</th>
      <th>Matcher 2</th>
      <th>Local</th>
      <th>Fecha</th>
      <th>Hora</th>
      <th>Match</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @meetings.each do |meeting| %>
      <tr>
        <td><%= meeting.matcher_1 %></td>
        <td><%= meeting.matcher_2 %></td>
        <td><%= meeting.local %></td>
        <td><%= meeting.fecha %></td>
        <td><%= meeting.hora %></td>
        <td><%= meeting.match %></td>
        <td><%= link_to 'Show', meeting %></td>
        <td><%= link_to 'Edit', edit_meeting_path(meeting) %></td>
        <td><%= link_to 'Destroy', meeting, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Meeting', new_meeting_path %>
