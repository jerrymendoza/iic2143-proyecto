<p id="notice"><%= notice %></p>

<h1>Locales</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Dueno</th>
      <th>Comuna</th>
      <th>Descripcion</th>
      <th>Aceptado</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @locals.each do |local| %>
      <tr>
        <td><%= local.nombre %></td>
        <td><%= local.dueno %></td>
        <td><%= local.comuna.name %></td>
        <td><%= local.descripcion %></td>
        <td><%= local.aceptado %></td>
        <td><%= link_to 'Detalles', local %></td>
        <td><%= link_to 'Edit', edit_local_path(local) %></td>
        <td><%= link_to 'Destroy', local, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Local', new_local_path %>
