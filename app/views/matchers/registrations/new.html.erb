<h2>Sign up Matcher</h2>

<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= render "devise/shared/error_messages", resource: resource %>

  <div class="field">
    <%= f.label :nombre %><br />
    <%= f.text_field :nombre, :class => 'uk-input uk-width-1-4' %>
  </div>

  <div class="field">
    <%= f.label :email %><br />
    <%= f.email_field :email, autofocus: true, autocomplete: "email", :class => 'uk-input uk-width-1-4' %>
  </div>

  <div class="field">
    <%= f.label :contraseña %>
    <% if @minimum_password_length %>
    <em>(<%= @minimum_password_length %> characters minimum)</em>
    <% end %><br />
    <%= f.password_field :password, autocomplete: "new-password", :class => 'uk-input uk-width-1-4' %>
  </div>

  <div class="field">
    <%= f.label :confirmar_contraseña %><br />
    <%= f.password_field :password_confirmation, autocomplete: "new-password", :class => 'uk-input uk-width-1-4' %>
  </div>

  <br>
  <div class="actions">
    <%= f.submit "Continuar", class: 'uk-button uk-button-primary' %>
  </div>
  </br>
<% end %>

<br>
<%= render "matchers/shared/links" %>
</br>
